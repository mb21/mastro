{
  "tasks": {
    "dev": "deno run --watch libs/server.ts",
    "generate": "deno run --allow-read=. --allow-write=dist --allow-net libs/generate.ts"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact"
  },
  "imports": {
    "$std/": "https://deno.land/std@0.210.0/",
    "preact": "npm:preact@^10.22.0",
    "preact-render-to-string": "npm:preact-render-to-string@^6.4.2",
    "signal-polyfill": "npm:signal-polyfill"
  }
}
